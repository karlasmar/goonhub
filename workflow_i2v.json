{
  "1": {
    "class_type": "Wan2CheckpointLoader",
    "inputs": {
      "ckpt_name_high_noise": "wan2.2_i2v_high_noise_14B_fp16.safetensors",
      "ckpt_name_low_noise": "wan2.2_i2v_low_noise_14B_fp16.safetensors",
      "vae_name": "wan_2.1_vae.safetensors"
    },
    "_meta": {
      "title": "Wan2 I2V Checkpoint Loader"
    }
  },
  "2": {
    "class_type": "LoadImageFromURL",
    "inputs": {
      "url": "IMAGE_URL_PLACEHOLDER"
    },
    "_meta": {
      "title": "Load Image from URL"
    }
  },
  "3": {
    "class_type": "Wan2TextEncode",
    "inputs": {
      "text": "PROMPT_PLACEHOLDER",
      "text_encoder": ["1", 1]
    },
    "_meta": {
      "title": "Positive Prompt"
    }
  },
  "4": {
    "class_type": "Wan2TextEncode",
    "inputs": {
      "text": "NEGATIVE_PROMPT_PLACEHOLDER",
      "text_encoder": ["1", 1]
    },
    "_meta": {
      "title": "Negative Prompt"
    }
  },
  "5": {
    "class_type": "Wan2ImageToVideo",
    "inputs": {
      "image": ["2", 0],
      "width": 1280,
      "height": 720,
      "frames": 90,
      "batch_size": 1
    },
    "_meta": {
      "title": "Image to Video Latent"
    }
  },
  "6": {
    "class_type": "Wan2Sampler",
    "inputs": {
      "model": ["1", 0],
      "positive": ["3", 0],
      "negative": ["4", 0],
      "latent_image": ["5", 0],
      "seed": -1,
      "steps": 30,
      "cfg": 4.0,
      "sampler_name": "dpmpp_2m",
      "scheduler": "karras",
      "sample_shift": 8,
      "motion_strength": 7,
      "denoise": 1.0
    },
    "_meta": {
      "title": "Wan2 I2V Sampler"
    }
  },
  "7": {
    "class_type": "Wan2VAEDecode",
    "inputs": {
      "samples": ["6", 0],
      "vae": ["1", 2]
    },
    "_meta": {
      "title": "VAE Decode"
    }
  },
  "8": {
    "class_type": "Wan2SaveVideo",
    "inputs": {
      "images": ["7", 0],
      "filename_prefix": "wan2_i2v_output",
      "fps": 24,
      "save_image": true,
      "pingpong": false,
      "save_output": true
    },
    "_meta": {
      "title": "Save I2V Video"
    }
  }
}